<?xml version="1.0"?>
<fest:template xmlns:fest="http://fest.mail.ru" context_name="json">
	<fest:include src="portal-headline.xml"/>
	<fest:include src="portal-menu.xml"/>
	<fest:include src="left-col.xml"/>
	<fest:include src="list.xml"/>
	<fest:set name="content">
		<fest:get name="headline"/>
		<fest:get name="headline-menu"/>
		<div class="page minwidth">
			<fest:get name="left-col"/>
			<div class="page__content">
				<div class="page__content__wrapper">
					<fest:get name="list"/>
				</div>
			</div>
		</div>
	</fest:set>
</fest:template>